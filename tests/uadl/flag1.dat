#
# Tests the use of a virtual flag.  A multiply grabs the MulFlag,
# preventing another multiply from starting until the first one completes.
#
= asm

	addi r1,r1,10
	addi r2,r2,20
	mullw r3,r1,r2
	mullw r4,r1,r2
	addi r3,r3,1
	addi r4,r4,1
	mullw r5,r3,r2
	mullw r6,r3,r4
  
= /asm

CORE n=:P

RD n=NIA      d=0x00000000

# <GEN>
MD n=Mem ra=0x00000000 d=0x3821000A	#	addi r1,r1,10
MD n=Mem ra=0x00000004 d=0x38420014	#	addi r2,r2,20
MD n=Mem ra=0x00000008 d=0x7C6111D6	#	mullw r3,r1,r2
MD n=Mem ra=0x0000000c d=0x7C8111D6	#	mullw r4,r1,r2
MD n=Mem ra=0x00000010 d=0x38630001	#	addi r3,r3,1
MD n=Mem ra=0x00000014 d=0x38840001	#	addi r4,r4,1
MD n=Mem ra=0x00000018 d=0x7CA311D6	#	mullw r5,r3,r2
MD n=Mem ra=0x0000001c d=0x7CC321D6	#	mullw r6,r3,r4
# </GEN>

TRACE


I ea=0x0 id=1
M n=Mem t=ifetch ea=0x0 ra=0x0 d=0x3821000a
INSTR op=0x3821000a					asm="addi r1,r1,10"
ITIME t=2
R n=GPR i=1 d=0x0000000a
CTIME t=5

I ea=0x4 id=2
M n=Mem t=ifetch ea=0x4 ra=0x4 d=0x38420014
INSTR op=0x38420014					asm="addi r2,r2,20"
ITIME t=3
R n=GPR i=2 d=0x00000014
CTIME t=6

I ea=0x8 id=3
M n=Mem t=ifetch ea=0x8 ra=0x8 d=0x7c6111d6
INSTR op=0x7c6111d6					asm="mullw r3,r1,r2"
ITIME t=6
R n=GPR i=3 d=0x000000c8
CTIME t=9

I ea=0xc id=4
M n=Mem t=ifetch ea=0xc ra=0xc d=0x7c8111d6
INSTR op=0x7c8111d6					asm="mullw r4,r1,r2"
ITIME t=9
R n=GPR i=4 d=0x000000c8
CTIME t=12

I ea=0x10 id=5
M n=Mem t=ifetch ea=0x10 ra=0x10 d=0x38630001
INSTR op=0x38630001					asm="addi r3,r3,1"
ITIME t=12
R n=GPR i=3 d=0x000000c9
CTIME t=15

I ea=0x14 id=6
M n=Mem t=ifetch ea=0x14 ra=0x14 d=0x38840001
INSTR op=0x38840001					asm="addi r4,r4,1"
ITIME t=13
R n=GPR i=4 d=0x000000c9
CTIME t=16

I ea=0x18 id=7
M n=Mem t=ifetch ea=0x18 ra=0x18 d=0x7ca311d6
INSTR op=0x7ca311d6					asm="mullw r5,r3,r2"
ITIME t=16
R n=GPR i=5 d=0x00000fb4
CTIME t=19

I ea=0x1c id=8
M n=Mem t=ifetch ea=0x1c ra=0x1c d=0x7cc321d6
INSTR op=0x7cc321d6					asm="mullw r6,r3,r4"
ITIME t=19
R n=GPR i=6 d=0x00009dd1
CTIME t=22

I ea=0x20 id=9
M n=Mem t=ifetch ea=0x20 ra=0x20 d=0x00000000
