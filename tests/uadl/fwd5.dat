#
# Simple test to make sure that fowarding of partial register values works.
#
= asm

	cror  5,6,7
	cror  3,4,5 # Read from FwdEX and CR
	crand 2,5,3 # Read from FwdEX and FwdMM

= /asm

# <GEN>
MD n=Mem ra=0x00000000 d=0x4CA63B82	#	cror  5,6,7
MD n=Mem ra=0x00000004 d=0x4C642B82	#	cror  3,4,5 # Read from FwdEX and CR
MD n=Mem ra=0x00000008 d=0x4C451A02	#	crand 2,5,3 # Read from FwdEX and FwdMM
# </GEN>

CORE n=:P

RD n=NIA      d=0x00000000
RD n=CR       d=0x03000000

TRACE

I ea=0x0 id=1
M n=Mem t=ifetch ea=0x0 ra=0x0 d=0x4ca63b82
INSTR op=0x4ca63b82                                     asm="cror 5,6,7"
R n=CR d=0x07000000
ITIME t=1
CTIME t=5

I ea=0x4 id=2
M n=Mem t=ifetch ea=0x4 ra=0x4 d=0x4c642b82
INSTR op=0x4c642b82                                     asm="cror 3,4,5"
R n=CR d=0x17000000
ITIME t=2
CTIME t=6

I ea=0x8 id=3
M n=Mem t=ifetch ea=0x8 ra=0x8 d=0x4c451a02
INSTR op=0x4c451a02                                     asm="crand 2,5,3"
ITIME t=3
R n=CR d=0x37000000
CTIME t=7

I ea=0xc id=4
M n=Mem t=ifetch ea=0xc ra=0xc d=0x00000000
INSTR op=0x00000000                                     asm="halt "
ITIME t=4
CTIME t=8

RESULTS

RD n=CR d=0x37000000
