#
# Fowarding path test: The addo is stalled even
# though it can get XER bits from addmeo b/c the
# addme has a bit it needs and is dispatched in
# the same cycle.
#
= asm

	addmeo r6,r6
	addi  r5,r5,30
	addi  r4,r4,20
	addi  r3,r3,10
	addme r2,r2
  addme. r1,r1

= /asm

# <GEN>
MD n=Mem ra=0x00000000 d=0x7CC605D4	#	addmeo r6,r6
MD n=Mem ra=0x00000004 d=0x38A5001E	#	addi  r5,r5,30
MD n=Mem ra=0x00000008 d=0x38840014	#	addi  r4,r4,20
MD n=Mem ra=0x0000000c d=0x3863000A	#	addi  r3,r3,10
MD n=Mem ra=0x00000010 d=0x7C4201D4	#	addme r2,r2
MD n=Mem ra=0x00000014 d=0x7C2101D5	#	addme. r1,r1
# </GEN>

CORE n=:P

RD n=NIA     d=0x0
RD n=XER     d=0xe0000000

RD n=GPR i=1 d=10
RD n=GPR i=2 d=20
RD n=GPR i=3 d=30
RD n=GPR i=4 d=40
RD n=GPR i=5 d=50
RD n=GPR i=6 d=60

TRACE

I ea=0x0 id=1
M n=Mem t=ifetch ea=0x0 ra=0x0 d=0x7cc605d4
INSTR op=0x7cc605d4					asm="addmeo r6,r6"
ITIME t=2
R n=GPR i=6 d=0x0000003c
R n=XER d=0xa0000000
CTIME t=5

I ea=0x4 id=2
M n=Mem t=ifetch ea=0x4 ra=0x4 d=0x38a5001e
INSTR op=0x38a5001e					asm="addi r5,r5,30"
ITIME t=2
R n=GPR i=5 d=0x00000050
CTIME t=5

I ea=0x8 id=3
M n=Mem t=ifetch ea=0x8 ra=0x8 d=0x38840014
INSTR op=0x38840014					asm="addi r4,r4,20"
ITIME t=3
R n=GPR i=4 d=0x0000003c
CTIME t=6

I ea=0xc id=4
M n=Mem t=ifetch ea=0xc ra=0xc d=0x3863000a
INSTR op=0x3863000a					asm="addi r3,r3,10"
ITIME t=3
R n=GPR i=3 d=0x00000028
CTIME t=6

I ea=0x10 id=5
M n=Mem t=ifetch ea=0x10 ra=0x10 d=0x7c4201d4
INSTR op=0x7c4201d4					asm="addme r2,r2"
ITIME t=4
R n=GPR i=2 d=0x00000014
R n=XER d=0xa0000000
CTIME t=7

I ea=0x14 id=6
M n=Mem t=ifetch ea=0x14 ra=0x14 d=0x7c2101d5
INSTR op=0x7c2101d5					asm="addme. r1,r1"
ITIME t=5
R n=CR d=0x50000000
R n=GPR i=1 d=0x0000000a
R n=XER d=0xa0000000
CTIME t=8

I ea=0x18 id=7
M n=Mem t=ifetch ea=0x18 ra=0x18 d=0x00000000
INSTR op=0x00000000					asm="halt "
ITIME t=8
