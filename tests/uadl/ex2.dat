#
# A more complicated exception example, with different types of exceptions
# occuring.
#
= asm

	addi r1,r1,1
	addi r1,r1,1
	tw 4,r0,r0
	addi r1,r1,1      # Make sure we've deallocated r1 during the exception.
  lwz r3,0x1000(r0) # Will generate an exception.
	stw r3,0x1004(r0) # Will generate an exception.
	addi r3,r3,10     # Make sure that we've deallocated r3.
	.long 0x0
	addi r2,r2,1
	mfspr r31,SRR0
	addi r31,r31,4
	mtspr SRR0,r31
	rfi
	
= /asm

