#
# Simple test to check serialized stores.  A serialized store should not
# complete until the store-buffer entry has finished.
#
= asm

	stw r1,0x1000(r0)
	stw r2,0x0(r10)

= /asm

CORE n=:P

RD n=NIA    d=0x0
RD n=HDBCR0 d=0x80000000

RD n=GPR i=1   d=0xdeadbeef
RD n=GPR i=2   d=0x12345678
RD n=GPR i=10  d=0x10004

# <GEN>
MD n=Mem ra=0x00000000 d=0x90201000	#	stw r1,0x1000(r0)
MD n=Mem ra=0x00000004 d=0x904A0000	#	stw r2,0x0(r10)
# </GEN>

TRACE

I ea=0x0 id=1
T n=TlbCam t=instr ea=0x0 ra=0x0  E=0x0 EPN=0x0 RPN=0x4 SIZE=0x4 SR=0x0 SW=0x0 SX=0x0 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x0 WIMG=0x0
M n=Mem t=ifetch ea=0x0 ra=0x0 d=0x90201000
INSTR op=0x90201000					asm="stw r1,4096(r0)"
D n=Mem t=write ea=0x1000 nb=4
T n=TlbFoo t=store ea=0x1000 ra=0x1000  EPN=0x4 RPN=0x4 SIZE=0x4
T n=TlbCam t=store ea=0x1000 ra=0x1000  E=0x0 EPN=0x0 RPN=0x4 SIZE=0x4 SR=0x0 SW=0x0 SX=0x0 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x0 WIMG=0x0
M n=Mem t=write ea=0x1000 ra=0x1000 d=0xdeadbeef
ITIME t=4
CTIME t=9

I ea=0x4 id=2
T n=TlbCam t=instr ea=0x4 ra=0x4  E=0x0 EPN=0x0 RPN=0x4 SIZE=0x4 SR=0x0 SW=0x0 SX=0x0 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x0 WIMG=0x0
M n=Mem t=ifetch ea=0x4 ra=0x4 d=0x904a0000
INSTR op=0x904a0000					asm="stw r2,0(r10)"
D n=Mem t=write ea=0x10004 nb=4
T n=TlbCam t=store ea=0x10004 ra=0x10004  E=0x0 EPN=0x0 RPN=0x4 SIZE=0x4 SR=0x0 SW=0x0 SX=0x0 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x0 WIMG=0x0
M n=Mem t=write ea=0x10004 ra=0x10004 d=0x12345678
ITIME t=5
CTIME t=14

I ea=0x8 id=3
T n=TlbCam t=instr ea=0x8 ra=0x8  E=0x0 EPN=0x0 RPN=0x4 SIZE=0x4 SR=0x0 SW=0x0 SX=0x0 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x0 WIMG=0x0
M n=Mem t=ifetch ea=0x8 ra=0x8 d=0x00000000
INSTR op=0x00000000					asm="halt "
ITIME t=8
CTIME t=15

RESULTS

MD n=Mem ra=0x0000000000001000 d=0xdeadbeef	#	(3735928559)
MD n=Mem ra=0x0000000000010004 d=0x12345678	#	(305419896)
