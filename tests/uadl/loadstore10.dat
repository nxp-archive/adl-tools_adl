#
# Test multi-word load/stores with a very wide cache interface.  Same as
# loadstore9.dat, but with caches disabled.  We should still see wide memory
# accesses.
#
= asm

	addi r1,r1,1
	addi r1,r1,1
	lmw  r24,0x1000(r0)
	stmw r24,0x2000(r0)

= /asm

# <GEN>
MD n=Mem ra=0x00000000 d=0x38210001	#	addi r1,r1,1
MD n=Mem ra=0x00000004 d=0x38210001	#	addi r1,r1,1
MD n=Mem ra=0x00000008 d=0xBB001000	#	lmw  r24,0x1000(r0)
MD n=Mem ra=0x0000000c d=0xBF002000	#	stmw r24,0x2000(r0)
# </GEN>

