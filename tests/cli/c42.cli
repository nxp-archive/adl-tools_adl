#
# Basic testing of 'mem' and 'memlist' options.
#

setmem 0x1000 0xdeadbeef 
setmem 0x1004 0x12345678 
setmem 0x1008 0xabcdefab 
setmem 0x100c 0x87654321 
setmem 0x1010 0xa5a5a5a5 
setmem 0x1014 0x2347989a 
setmem 0x1018 0xabcd3576 
setmem 0x101c 0x897866aa 

setmem 0x2000 0x68656c6c
setmem 0x2004 0x6f2c2077
setmem 0x2008 0x6f726c64
setmem 0x200c 0x210a0000

puts [mem 0x1000 32]
puts [mem 0x1000 32 8]
puts [mem 0x1000 32 4]
puts [mem 0x1000 32 2]
puts [mem 0x1000 32 1 8]

puts [mem 0x1002 32]

puts "\n0x1000:  [memlist 0x1000 32]"
puts "\n0x1000:  [memlist 0x1000 32 8]"
puts "\n0x1000:  [memlist 0x1000 32 4]"
puts "\n0x1000:  [memlist 0x1000 32 2]"
puts "\n0x1000:  [memlist 0x1000 32 1]"
puts "\n0x2000:  [memlist 0x2000 32 1 char]"

quit
