#
# Test that miss handlers work for when they're defined at the MMU scope.
#
= asm

  addi r1,r1,1
  addi r1,r1,1
  lwz r2,0x1000(r0)  # Should miss and cause an exception.
  stw r1,0x1010(r0)  # Should miss and cause an exception.
  addi r1,r1,1
  addi r1,r1,1
  b 0x2000           # Target of branch should miss.
  .long 0x0
  addi r29,r29,1     # DTLBs bump r31.
  mfspr r20,SRR0
  addi  r20,r20,4
  mtspr SRR0,r20
  rfi
  .long 0x0
  addi r28,r28,1     # ITLBs bump r30 and don't return.
  .long 0x0

= /asm

