#
# Test that delayed writes to memory work.
#

= asm

	stw r1,0x1000(r0)  # Delayed write.
	lwz r3,0x1000(r0)
	lwz r4,0x1000(r0)
	lwz r5,0x1000(r0)
	lwz r6,0x1000(r0)
	lwz r7,0x1000(r0)
	stwx r1,r0,r30     # Immediate write.
	stw  r2,0x1010(r0) # Delayed write.
	lwz r10,0x1010(r0)
	lwz r11,0x1010(r0)
	lwz r12,0x1010(r0)
	lwz r13,0x1010(r0)
	lwz r14,0x1010(r0)

= /asm

#
# <GEN>
MD n=Mem ra=0x00000000 d=0x90201000	#	stw r1,0x1000(r0)  # Delayed write.
MD n=Mem ra=0x00000004 d=0x80601000	#	lwz r3,0x1000(r0)
MD n=Mem ra=0x00000008 d=0x80801000	#	lwz r4,0x1000(r0)
MD n=Mem ra=0x0000000c d=0x80A01000	#	lwz r5,0x1000(r0)
MD n=Mem ra=0x00000010 d=0x80C01000	#	lwz r6,0x1000(r0)
MD n=Mem ra=0x00000014 d=0x80E01000	#	lwz r7,0x1000(r0)
MD n=Mem ra=0x00000018 d=0x7C20F12E	#	stwx r1,r0,r30     # Immediate write.
MD n=Mem ra=0x0000001c d=0x90401010	#	stw  r2,0x1010(r0) # Delayed write.
MD n=Mem ra=0x00000020 d=0x81401010	#	lwz r10,0x1010(r0)
MD n=Mem ra=0x00000024 d=0x81601010	#	lwz r11,0x1010(r0)
MD n=Mem ra=0x00000028 d=0x81801010	#	lwz r12,0x1010(r0)
MD n=Mem ra=0x0000002c d=0x81A01010	#	lwz r13,0x1010(r0)
MD n=Mem ra=0x00000030 d=0x81C01010	#	lwz r14,0x1010(r0)
# </GEN>

CORE n=:P

RD n=NIA d=0x0

RD n=GPR i=1  d=0xdeadbeef
RD n=GPR i=2  d=0x12345678
RD n=GPR i=30 d=0x1010

TRACE

I ea=0x0 id=1 tic=1
M n=Mem t=ifetch ea=0x0 ra=0x0 d=0x90201000
INSTR op=0x90201000					asm="stw r1,4096(r0)"

I ea=0x4 id=2 tic=2
M n=Mem t=ifetch ea=0x4 ra=0x4 d=0x80601000
INSTR op=0x80601000					asm="lwz r3,4096(r0)"
D n=Mem t=read ea=0x1000 nb=4
M n=Mem t=read ea=0x1000 ra=0x1000 d=0x00000000
R n=GPR i=3 d=0x00000000

