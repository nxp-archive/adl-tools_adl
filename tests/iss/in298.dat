#
# Make sure that slice-writes register as targets.  We use lbz in this case,
# where mod89 was modified so that lbz uses a slice-write.
#
= asm

	lbz r1,0x1000(r0)
	addi r2,r1,100
	addi r3,r3,20
	stw r2,0x2000(r0)
	stw r5,0x2004(r0)

= /asm

# <GEN>
MD n=Mem ra=0x00000000 d=0x88201000	#	lbz r1,0x1000(r0)
