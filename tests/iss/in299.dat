#
# Another time-tagged ISS test testing load latencies in a dual-issue machine.
#

= asm

	lwz r1,0x1000(r0)
	add r3,r2,r1
	lwz r2,0x1004(r0)
	add r4,r2,r3

= /asm

# <GEN>
MD n=Mem ra=0x00000000 d=0x80201000	#	lwz r1,0x1000(r0)
MD n=Mem ra=0x00000004 d=0x7C620A14	#	add r3,r2,r1
MD n=Mem ra=0x00000008 d=0x80401004	#	lwz r2,0x1004(r0)
MD n=Mem ra=0x0000000c d=0x7C821A14	#	add r4,r2,r3
# </GEN>

CORE n=:P

MD n=Mem ra=0x1000 d=10
MD n=Mem ra=0x1004 d=20

TRACE

I ea=0x0 id=1 tic=1
M n=Mem t=ifetch ea=0x0 ra=0x0 d=0x80201000
INSTR op=0x80201000					asm="lwz r1,4096(r0)"
D n=Mem t=read ea=0x1000 nb=4
C n=L1d a=miss t=read set=32 way=0 lm=0x7f ra=0x1000
C n=L1d a=load t=read set=32 way=0 lm=0x7f ra=0x1000
M n=Mem t=read ea=0x1000 ra=0x1000 d=0x0000000a
R n=GPR i=1 d=0x0000000a
ITIME t=11
CTIME t=24

I ea=0x4 id=2 tic=2
M n=Mem t=ifetch ea=0x4 ra=0x4 d=0x7c620a14
INSTR op=0x7c620a14					asm="add r3,r2,r1"
R n=GPR i=3 d=0x0000000a
ITIME t=22
CTIME t=25

I ea=0x8 id=3 tic=3
M n=Mem t=ifetch ea=0x8 ra=0x8 d=0x80401004
INSTR op=0x80401004					asm="lwz r2,4100(r0)"
D n=Mem t=read ea=0x1004 nb=4
C n=L1d a=hit t=read set=32 way=0 lm=0x7f ra=0x1000
M n=Mem t=read ea=0x1004 ra=0x1004 d=0x00000014
R n=GPR i=2 d=0x00000014
ITIME t=22
CTIME t=25

I ea=0xc id=4 tic=4
M n=Mem t=ifetch ea=0xc ra=0xc d=0x7c821a14
INSTR op=0x7c821a14					asm="add r4,r2,r3"
R n=GPR i=4 d=0x0000001e
ITIME t=23
CTIME t=26

I ea=0x10 id=5 tic=5
M n=Mem t=ifetch ea=0x10 ra=0x10 d=0x00000000
INSTR op=0x00000000					asm="halt "
ITIME t=23
CTIME t=26

RESULTS

RD n=GPR i=1 d=10
RD n=GPR i=2 d=20
RD n=GPR i=3 d=10
RD n=GPR i=4 d=30
