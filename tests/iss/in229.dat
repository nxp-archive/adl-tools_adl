#
# Check that we can retry a translation on a miss, after adding to the TLB.
#
= asm

	addi r1,r1,1
	lwz r2,0x1000(r0)
	lwz r3,0x1004(r0)

= /asm

CORE n=:P

# ea=0x0000 ra=0x0000
TD n=TlbCam set=0 way=0 V=1 TID=0 SIZE=1 TS=0 RPN=0 WIMG=0x1 SX=1 SR=1 SW=1 EPN=0

MD n=Mem ra=0xa1000 d=0xdeadbeef
MD n=Mem ra=0xa1004 d=0x12345678

# On a data-miss, take data from MAS regs.
RD n=HDBCR0 d=0x40000000

# These MAS regs map 0x1000 -> 0xa1000 using the 4k.
RD n=MAS0 d=0x00000402
RD n=MAS1 d=0x00028400
RD n=MAS2 d=0xfc000010

#
# <GEN>
MD n=Mem ra=0x00000000 d=0x38210001	#	addi r1,r1,1
MD n=Mem ra=0x00000004 d=0x80401000	#	lwz r2,0x1000(r0)
MD n=Mem ra=0x00000008 d=0x80601004	#	lwz r3,0x1004(r0)
# </GEN>

TRACE

I ea=0x0 id=1
T n=TlbCam t=instr set=0 way=0 ea=0x0 ra=0x0  EPN=0x0 RPN=0x0 SIZE=0x1 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x1 WIMG=0x1
M n=Mem t=ifetch ea=0x0 ra=0x0 d=0x38210001
INSTR op=0x38210001					asm="addi r1,r1,1"
R n=GPR i=1 d=0x00000001

I ea=0x4 id=2
T n=TlbCam t=instr set=0 way=0 ea=0x4 ra=0x4  EPN=0x0 RPN=0x0 SIZE=0x1 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x1 WIMG=0x1
M n=Mem t=ifetch ea=0x4 ra=0x4 d=0x80401000
INSTR op=0x80401000					asm="lwz r2,4096(r0)"
D n=Mem t=read ea=0x1000 nb=4
T n=Tlb4k t=write set=1 way=0  NV=0x0 EPN=0x4 RPN=0x284 SIZE=0x0 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x1 UW=0x1 UX=0x1 V=0x1 WIMG=0x0
T n=Tlb4k t=load set=1 way=0 ea=0x1000 ra=0xa1000  NV=0x0 EPN=0x4 RPN=0x284 SIZE=0x0 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x1 UW=0x1 UX=0x1 V=0x1 WIMG=0x0
M n=Mem t=read ea=0x1000 ra=0xa1000 d=0xdeadbeef
R n=GPR i=2 d=0xdeadbeef

I ea=0x8 id=3
T n=TlbCam t=instr set=0 way=0 ea=0x8 ra=0x8  EPN=0x0 RPN=0x0 SIZE=0x1 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x1 WIMG=0x1
M n=Mem t=ifetch ea=0x8 ra=0x8 d=0x80601004
INSTR op=0x80601004					asm="lwz r3,4100(r0)"
D n=Mem t=read ea=0x1004 nb=4
T n=Tlb4k t=load set=1 way=0 ea=0x1004 ra=0xa1004  NV=0x0 EPN=0x4 RPN=0x284 SIZE=0x0 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x1 UW=0x1 UX=0x1 V=0x1 WIMG=0x0
M n=Mem t=read ea=0x1004 ra=0xa1004 d=0x12345678
R n=GPR i=3 d=0x12345678

I ea=0xc id=4
T n=TlbCam t=instr set=0 way=0 ea=0xc ra=0xc  EPN=0x0 RPN=0x0 SIZE=0x1 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x1 WIMG=0x1
M n=Mem t=ifetch ea=0xc ra=0xc d=0x00000000
INSTR op=0x00000000					asm="halt "

RESULTS

RD n=GPR i=2 d=0xdeadbeef
RD n=GPR i=3 d=0x12345678

TD n=TlbCam set=0 way=0 EPN=0x0 RPN=0x0 SIZE=0x1 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x1 WIMG=0x1
TD n=Tlb4k set=1 way=0 NV=0x0 EPN=0x4 RPN=0x284 SIZE=0x0 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x1 UW=0x1 UX=0x1 V=0x1 WIMG=0x0
