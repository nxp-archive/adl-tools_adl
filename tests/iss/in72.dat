#
# This tests the use of memory hooks to implement little-endian mode,
# driven by translation attributes.
#

= asm

 lwz  r2,0x1000(0)
 add  r3,r2,r1
 subic. r5,r5,2
 stw  r3,0(r4)       # Write to a little-endian page.
 stw  r2,0xff0(r0)   # Write to a big-endian page.
 stw  r3,0xfff(r4)   # Write to a little-endian page.
 lhz  r6,0xfff(r4)   # Load from a little-endian page.
 lhz  r7,0xff2(r0)   # Load from a big-endian page.

= /asm

= mdopts

 instr-offset: 0x10000

= /mdopts

# <GEN>
MD n=Mem ra=0x00010000 d=0x80401000	#	lwz  r2,0x1000(0)
MD n=Mem ra=0x00010004 d=0x7C620A14	#	add  r3,r2,r1
MD n=Mem ra=0x00010008 d=0x34A5FFFE	#	subic. r5,r5,2
MD n=Mem ra=0x0001000c d=0x90640000	#	stw  r3,0(r4)       # Write to a little-endian page.
MD n=Mem ra=0x00010010 d=0x90400FF0	#	stw  r2,0xff0(r0)   # Write to a big-endian page.
MD n=Mem ra=0x00010014 d=0x90640FFF	#	stw  r3,0xfff(r4)   # Write to a little-endian page.
MD n=Mem ra=0x00010018 d=0xA0C40FFF	#	lhz  r6,0xfff(r4)   # Load from a little-endian page.
MD n=Mem ra=0x0001001c d=0xA0E00FF2	#	lhz  r7,0xff2(r0)   # Load from a big-endian page.
