#
# Basic test of the MMU.  We test that we're masking the RPN
# for bits which would fall into the offset of a translated address.
#

= asm

lwz  r2,0x1000(0)
stw  r2,0x2000(0)

= /asm

= mdopts

instr-offset: 0x10000

= /mdopts

#
# <GEN>
MD n=Mem ra=0x00010000 d=0x80401000	#	lwz  r2,0x1000(0)
MD n=Mem ra=0x00010004 d=0x90402000	#	stw  r2,0x2000(0)
# </GEN>
CORE n=:P
MD n=Mem ra=0xa1000 d=0xdeadbeef

# ea=0x0000 ra=0x10000
TD n=TlbCam set=0 way=0 V=1 TID=0 SIZE=1 TS=0 RPN=0x41 WIMG=0x1 SX=1 SR=1 SW=1 EPN=0

# ea=0x1000 ra=0xA1000
TD n=TlbCam set=0 way=1 V=1 TID=0 SIZE=1 TS=0 RPN=0x285 WIMG=0x2 SX=1 SR=1 SW=1 EPN=0x4

# ea=0x2000 ra=0xB2000
TD n=TlbCam set=0 way=2 V=1 TID=0 SIZE=1 TS=0 RPN=0x2c9 WIMG=0x3 SX=1 SR=1 SW=1 EPN=0x8

RD n=NIA d=0x00000000

TRACE

I ea=0x0 id=1
R n=HitReg d=0x00000001
T n=TlbCam t=instr  E=0x0 EPN=0x0 RPN=0x41 SIZE=0x1 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x1 WIMG=0x1
M n=Mem t=ifetch ea=0x0 ra=0x10000 d=0x80401000
INSTR op=0x80401000					asm="lwz r2,4096(r0)"
R n=NIA d=0x00000004
D n=Mem t=read ea=0x1000 nb=4
R n=HitReg d=0x00000002
T n=TlbCam t=load  E=0x0 EPN=0x4 RPN=0x285 SIZE=0x1 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x1 WIMG=0x2
M n=Mem t=read ea=0x1000 ra=0xa1000 d=0xdeadbeef
R n=GPR i=2 d=0xdeadbeef

I ea=0x4 id=2
T n=TlbCam t=instr  E=0x0 EPN=0x0 RPN=0x41 SIZE=0x1 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x1 WIMG=0x1
M n=Mem t=ifetch ea=0x4 ra=0x10004 d=0x90402000
INSTR op=0x90402000					asm="stw r2,8192(r0)"
R n=NIA d=0x00000008
D n=Mem t=write ea=0x2000 nb=4
R n=HitReg d=0x00000003
T n=TlbCam t=store  E=0x0 EPN=0x8 RPN=0x2c9 SIZE=0x1 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x1 WIMG=0x3
M n=Mem t=write ea=0x2000 ra=0xb2000 d=0xdeadbeef

I ea=0x8 id=3
T n=TlbCam t=instr  E=0x0 EPN=0x0 RPN=0x41 SIZE=0x1 SR=0x1 SW=0x1 SX=0x1 TID=0x0 TS=0x0 UR=0x0 UW=0x0 UX=0x0 V=0x1 WIMG=0x1
M n=Mem t=ifetch ea=0x8 ra=0x10008 d=0x00000000
INSTR op=0x00000000					asm="halt "
R n=NIA d=0x0000000c

RESULTS

RD n=GPR i=2 d=0xdeadbeef

MD n=Mem ra=0xa1000 d=0xdeadbeef
MD n=Mem ra=0xb2000 d=0xdeadbeef
