M n=Mem t=read ea=0x1010 ra=0x1010 d=0xa5ef4234
M n=Mem t=read ea=0x1014 ra=0x1014 d=0x12349877
M n=Mem t=read ea=0x1018 ra=0x1018 d=0xacdbfeaf
M n=Mem t=read ea=0x101c ra=0x101c d=0x2346854a
