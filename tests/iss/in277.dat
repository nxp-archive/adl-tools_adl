I ea=0xc id=4
M n=Mem t=ifetch ea=0xc ra=0xc d=0x38420014
INSTR op=0x38420014					asm="addi r2,r2,20"
R n=GPR i=2 d=0x0000002c

I ea=0x10 id=5
M n=Mem t=ifetch ea=0x10 ra=0x10 d=0x7c8111ce
INSTR op=0x7c8111ce					asm="stvx r4,r1,r2"
D n=Mem t=write ea=0x102c nb=16 type=write
M n=Mem t=write ea=0x1020 ra=0x1020 d=0xdeadbeef
