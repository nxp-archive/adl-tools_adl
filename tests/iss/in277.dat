INSTR op=0x81810000 asm="lwz r12,0(r1)"
D n=Mem t=read ea=0x1000 nb=4
M n=Mem t=read ea=0x1000 ra=0x1000 d=0xdeadbeef
R n=GPR i=12 d=0xdeadbeef

I ea=0x18
M n=Mem t=ifetch ea=0x18 ra=0x18 d=0x91802000
INSTR op=0x91802000 asm="stw r12,8192(r0)"
