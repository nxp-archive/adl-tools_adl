#
# This tests to make sure that we properly log a critical address in the D card.
# In this situation, lvx and stvx log their original, unaligend address in the D
# card, rather than the actual starting address.
#
= asm

	lvx r4,r1,r2
	addi r2,r2,0x14
	lvx r8,r1,r2

	addi r2,r2,0x14
	stvx r4,r1,r2

	lwz r12,0x0(r1)
	stw r12,0x2000(r0)

= /asm

= aopts
	-mregnames -maltivec
= /aopts

# <GEN>
MD n=Mem ra=0x00000000 d=0x7C8110CE	#	lvx r4,r1,r2
MD n=Mem ra=0x00000004 d=0x38420014	#	addi r2,r2,0x14
MD n=Mem ra=0x00000008 d=0x7D0110CE	#	lvx r8,r1,r2
MD n=Mem ra=0x0000000c d=0x38420014	#	addi r2,r2,0x14
MD n=Mem ra=0x00000010 d=0x7C8111CE	#	stvx r4,r1,r2
MD n=Mem ra=0x00000014 d=0x81810000	#	lwz r12,0x0(r1)
MD n=Mem ra=0x00000018 d=0x91802000	#	stw r12,0x2000(r0)
# </GEN>

CORE n=:P

RD n=NIA    d=0x0

RD n=GPR i=1 d=0x1000
RD n=GPR i=2 d=0x4


MD n=Mem ra=0x1000 d=0xdeadbeef
MD n=Mem ra=0x1004 d=0x12345678
MD n=Mem ra=0x1008 d=0x87654321
MD n=Mem ra=0x100c d=0xb0f0babe

MD n=Mem ra=0x1010 d=0xa5ef4234
MD n=Mem ra=0x1014 d=0x12349877
MD n=Mem ra=0x1018 d=0xacdbfeaf
MD n=Mem ra=0x101c d=0x2346854a

TRACE

I ea=0x0 id=1
M n=Mem t=ifetch ea=0x0 ra=0x0 d=0x7c8110ce
INSTR op=0x7c8110ce					asm="lvx r4,r1,r2"
D n=Mem t=read ea=0x1004 nb=16 type=read
M n=Mem t=read ea=0x1000 ra=0x1000 d=0xdeadbeef
